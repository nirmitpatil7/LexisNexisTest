{
  "name": "technical-test",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "wdio run ./wdio.conf.js",
    "test:desktop": "wdio run ./config/wdio.desktop.conf.js",
    "test:mobile": "wdio run ./config/wdio.mobile.conf.js",
    "test:api": "wdio run ./config/wdio.api.conf.js",
    "test:api:create": "wdio run ./config/wdio.api.conf.js --spec=**/createBooking.spec.js",
    "test:api:get": "wdio run ./config/wdio.api.conf.js --spec=**/getBooking.spec.js",
    "test:api:update": "wdio run ./config/wdio.api.conf.js --spec=**/updateBooking.spec.js",
    "test:api:delete": "wdio run ./config/wdio.api.conf.js --spec=**/deleteBooking.spec.js",
    "test:all": "npm run test:desktop && npm run test:mobile && npm run test:api",
    "appium": "appium",
    "appium:start": "appium --address 127.0.0.1 --port 4723 --log-level info"
  },
